{
  "name": "crappwebapi",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prestart": "node aspnetcore-https && node aspnetcore-vue",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "firebase": "nuxt build && npm run copy && npm install",
    "clean": "npm run clean:public && npm run clean:static",
    "clean:public": "rm -rf \"public/**/*.*!(md)\" && rm -rf \"public/_nuxt\"",
    "clean:static": "rm -rf \"static/sw.js\"",
    "copy": "npm run copy:nuxt && npm run copy:static",
    "copy:nuxt": "xcopy \".nuxt\\dist\\*\" \"public\\_nuxt\\\" /E /Y",
    "copy:static": "xcopy \"static\\*\" \"public\\\" /E /Y",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "serve": "nuxt serve",
    "start:firebase": "firebase emulators:start",
    "deploy": "firebase deploy --only hosting:staging",
    "deploy:firebase": "firebase deploy --only hosting:live",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-geocoder": "^4.5.1",
    "@nuxt/webpack": "^2.15.5",
    "@nuxtjs/auth-next": "5.0.0-1643791578.532b3d6",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.32.0",
    "animate.css": "^4.1.1",
    "chart.js": "2.9.4",
    "compress.js": "^1.2.1",
    "dotenv": "^8.2.0",
    "file-saver": "^2.0.5",
    "image-conversion": "^2.1.1",
    "jspdf": "^1.4.1",
    "jszip": "^3.5.0",
    "jwt-decode": "^3.1.2",
    "mapbox-gl": "^1.13.0",
    "moment": "^2.29.1",
    "nuxt": "^2.15.7",
    "signature_pad": "^3.0.0-beta.4",
    "v-mask": "^2.2.4",
    "vee-validate": "^3.4.5",
    "vue-chartjs": "^3.5.1",
    "vue-html2pdf": "^1.8.0",
    "vue-imask": "^6.1.0",
    "vue-lazy-hydration": "^2.0.0-beta.4",
    "vue-signature-pad": "^2.0.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/vuetify": "^1.11.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-vue": "^7.5.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.6",
    "sass-loader": "^10.1.1"
  }
}
